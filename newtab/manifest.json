{
  "name": "LaunchTab",
  "version": "1.0",
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
   "history", "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_icon": "sample-16x16.png",
      "default_title": "LaunchTab",
      "popup" : "popT.html"
  },
  "icons": {
            "128": "sample_icon_128.png"
  }
}
